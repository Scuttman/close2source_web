rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /profile-pics/{userId} {
      allow write: if request.auth != null && request.auth.uid == userId;
      allow read: if true;
    }
    match /projects/{allPaths=**} {
      allow write: if request.auth != null;
      allow read: if true;
    }
  }
}
